struct Vec3 {
  float x;
  float y;
  float z;
};


struct Mat3x3 {
  struct Vec3 x;
  struct Vec3 y;
  struct Vec3 z;
};

int glob1 = 2; 

struct Vec3 incr_if_cond(struct Vec3 v, bool pcond){
  struct Vec3 res;

  if(pcond){
    res.x = v.x + glob1;
    res.y = v.y + glob1;
    res.z = v.z + glob1; 
  } else {
    res.x = v.x - glob1;
    res.y = v.y - glob1;
    res.z = v.z - glob1; 
  }
 
  return res;
}

int main() {
  
  struct Vec3* alpha; 
  struct Mat3x3 beta;
  bool cond = true; 

  (*alpha).x = 1.;
  (*alpha).y = 2.;
  (*alpha).z = 3.;

  
  beta.x = *alpha;
  beta.y = incr_if_cond(*alpha, cond);
  beta.z = incr_if_cond(beta.y, cond);

  putchar((int)beta.y.z);
  putchar((int)(*alpha).z);

  return 0;
}


